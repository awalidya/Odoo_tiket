<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <template id="report_pesawat">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as = "doc">
                <t t-call="web.external_layout">
                    <h2>Tiket_Card</h2>
                        Nama: <t t-esc="doc.name"/>
                        Tanggal: <t t-esc="doc.tanggal"/>
                        Maskapai:<t t-esc="doc.maskapai"/>
                        Keberangkatan:<t t-esc="doc.kota_keberangkatan"/>
                        Tujuan:<t t-esc="doc.kota_tujuan"/> 
                </t>
            </t>
        </t>
    </template>
</odoo>
